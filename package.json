{
  "name": "xtendui",
  "version": "1.0.0-beta.5",
  "description": "UI framework for custom styling, interactions and animations.",
  "homepage": "http://xtendui.com/",
  "license": "MIT",
  "author": "Riccardo Caroli <beaver82@gmail.com>",
  "repository": "https://github.com/minimit/xtendui.git",
  "keywords": [
    "frontend",
    "library",
    "framework",
    "javascript",
    "tailwindcss",
    "animation",
    "ui"
  ],
  "main": "src/xt.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "tailwind.*.js",
    "dist/*.min.*",
    "src/*.*",
    "src/addons/*.*",
    "src/core/*.*",
    "src/future/*.*"
  ],
  "scripts": {
    "webpack": "webpack --progress",
    "build": "gatsby clean && gatsby build",
    "dev": "gatsby clean && gatsby develop -p 8080",
    "serve": "yarn build && gatsby serve -p 8080",
    "deploy": "yarn build && gh-pages -d public",
    "release": "npx semantic-release --dry-run --no-ci",
    "lint": "eslint . --ext .js",
    "postversion": "yarn webpack"
  },
